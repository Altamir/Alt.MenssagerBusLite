    android:layout_width="match_parent"
    android:layout_height="match_parent">

	<Button
		android:id="@+id/bt_clear"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="limpar"/>

	<Button
		android:id="@+id/bt_voltar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="voltar"/>

	<EditText
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:gravity="bottom"		
		android:text="log"
		android:id="@+id/ed_log" />

</LinearLayout>
